{
    "name": "telemetry",
    "version": "1.0.0",
    "description": "Telemery event data",
    "main": "telemetry.js",
    "scripts": {
        "build": "npm run lint && webpack",
        "build:dist": "npm run build && npm run build:server",
        "build:server": "tsc -p ./server/tsconfig-server.json",
        "build:watch": "webpack --watch",
        "build:testBundles": "webpack --config webpack.config.test.js",
        "build:testBundles:watch": "webpack --config webpack.config.test.js --watch",
        "build:docker": "docker build -t telemetry:latest .",
        "deploy": "npm run build && npm run test",
        "start:dev": "webpack-dev-server",
        "start:devTest": "webpack-dev-server --config webpack.config.test.js -d",
        "start:docker": "NODE_ENV=development docker run -p 3000:80 telemetry",
        "test": "mocha -r ts-node/register test/**/*.spec.ts",
        "lint": "tslint --project ./"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:christopher-lawrence/telemetry.git"
    },
    "author": "Chris Lawrence",
    "license": "ISC",
    "devDependencies": {
        "@types/chai": "^4.0.6",
        "@types/cookie_js": "^1.2.0",
        "@types/express": "^4.11.0",
        "@types/jquery": "^3.2.16",
        "@types/jsdom": "^11.0.4",
        "@types/mocha": "^2.2.44",
        "@types/node": "^9.4.0",
        "@types/sinon": "^4.0.0",
        "@types/underscore": "^1.8.5",
        "chai": "^4.1.2",
        "cookie_js": "^1.2.2",
        "inline-source-map": "^0.6.2",
        "jquery": "^3.2.1",
        "jsdom": "^11.5.1",
        "jsdom-global": "^3.0.2",
        "mocha": "^4.0.1",
        "sinon": "^4.1.2",
        "ts-loader": "^3.1.1",
        "ts-node": "^3.3.0",
        "tslint": "^5.8.0",
        "typescript": "^2.6.2",
        "underscore": "^1.8.3",
        "webpack": "^3.8.1",
        "webpack-dev-server": "^2.9.4"
    },
    "dependencies": {
        "aws-sdk": "^2.188.0",
        "body-parser": "^1.18.2",
        "express": "^4.16.2"
    }
}
