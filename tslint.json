{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "ordered-imports": false,
        "quotemark": [
            true,
            "single"
        ],
        "one-variable-per-declaration": false,
        "no-unused-expression": false,
        "member-ordering": [
            true,
            "fields-first"
        ],
        "object-literal-shorthand": false,
        "forin": false,
        "prefer-template": true,
        "curly": true,
        "space-before-function-paren": false
    },
    "rulesDirectory": []
}
